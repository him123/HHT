<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="@dimen/h3"
        android:layout_marginRight="@dimen/h3"
        android:layout_marginTop="@dimen/h2"
        android:background="@android:color/transparent">

        <!--Main Body of your custom dialog-->


        <Button
            android:id ="@+id/btnPrint"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginTop="@dimen/h2"
            android:layout_marginLeft="@dimen/image_width"

            android:drawableTop="@drawable/prints"
            android:background="@drawable/rounded_buttons"
            android:padding="@dimen/h2"
            android:text="@string/print"

            android:drawablePadding="0dp"
            />


        <Button
            android:id ="@+id/btnCancel2"
            android:layout_toRightOf="@+id/btnPrint"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:text="@string/do_not_print"

            android:layout_marginTop="@dimen/h2"
            android:drawableTop="@drawable/dontprints"
            android:layout_marginLeft="@dimen/h2"
            android:background="@drawable/rounded_buttons"
            android:padding="@dimen/h2"
            android:drawablePadding="0dp"



            />


    </RelativeLayout>


</RelativeLayout>